{
  "name": "railroad/crux",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "~7",
    "laravel/framework": "5.*",
    "guzzlehttp/guzzle": "~6.0",
    "railroad/ecommerce": "4.0.*",
    "railroad/usora": "^2.0.83"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "phpunit/php-code-coverage": "~6.0",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Crux\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Crux\\Tests\\": "tests/"
    }
  }
}
