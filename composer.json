{
  "name": "railroad/crux",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8",
    "laravel/framework": "^9.9",
    "guzzlehttp/guzzle": "^7.2",
    "railroad/ecommerce": "^5.0",
    "railroad/usora": "3.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "phpunit/php-code-coverage": "~6.0",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Crux\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Crux\\Tests\\": "tests/"
    }
  }
}
